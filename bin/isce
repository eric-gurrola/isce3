# main entry point; protect all this to make __main__ importable
if __name__ == '__main__':
    # get the package
    import isce
    # build an instance of the plexus
    app = isce.shells.isce(name='isce.plexus')
    # run it
    status = app.run()
    # and report the exit code to the shell
    raise SystemExit(status)

# end of file
